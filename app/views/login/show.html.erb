<h1>Welcome, would you like to login or sign up?</h1>

<section>
  <%= link_to Sign Up, new_user_path %>
  <p>Log In:</p>
  <%= form_for @user do |f|%>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.submit %>

  <% if @user.authenticate %>

</section>
